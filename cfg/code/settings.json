{
    "window.zoomLevel": 0.5,
    "workbench.colorTheme": "Gruvbox Dark Hard",
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontFamily": "Dank Mono, Operator Mono, Fira Code, monospace",
    "editor.fontSize": 14,
    "editor.fontLigatures": true,
    "editor.lineNumbers": "relative",
    "terminal.integrated.fontFamily": "SauceCodePro Nerd Font, FiraCode Nerd Font, monospace",
    "terminal.integrated.fontSize": 13,
    "workbench.activityBar.visible": false,
    "git.allowForcePush": true,
    "git.enableCommitSigning": true,
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.fetchOnPull": true,
    "editor.tabSize": 2,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.insertFinalNewline": true,
    "files.associations": {
        ".vimrc": "viml",
        "*Dockerfile": "dockerfile"
    },
    "vim.leader": "<space>",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [ "<leader>", "<leader>", "f" ],
            "commands": [ "editor.action.formatDocument" ]
        },
        {
            "before": [ "_" ],
            "commands": [ "workbench.action.previousEditor" ]
        },
        {
            "before": [ "+" ],
            "commands": [ "workbench.action.nextEditor" ]
        }
    ],
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.color": "rgba(64, 64, 64, 1.0)",
    "vim.easymotion": true,
    "vim.easymotionMarkerFontFamily": "Dank Mono, Operator Mono, Fira Code, monospace",
    "vim.easymotionMarkerYOffset": 14,
    "vim.easymotionMarkerHeight": 16,
    "vim.easymotionMarkerFontSize": "13",
    "vim.surround": true,
    "go.useLanguageServer": true,
    "cSpell.userWords": [
        "archanan",
        "toolchain"
    ]
}
